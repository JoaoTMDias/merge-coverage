{
  "name": "@jtmdias/merge-coverage",
  "version": "0.0.0",
  "description": "Merges test coverage between different test frameworks",
  "author": "JoÃ£o Dias @JoaoTMDias",
  "bin": {
    "merge-coverage": "./bin/run"
  },
  "homepage": "https://github.com/JoaoTMDias/merge-coverage",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "JoaoTMDias/merge-coverage",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@jtmdias/js-utilities": "^1.2.0",
    "@oclif/test": "^2.3.16",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.16.1",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "istanbul-lib-coverage": "^3.2.0",
    "mocha": "^9",
    "nyc": "^15.1.0",
    "oclif": "^3",
    "semantic-release": "^20.1.1",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "merge-coverage",
    "dirname": "merge-coverage",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "clean:dist": "shx rm -rf dist",
    "clean:manifest": "shx rm -f oclif.manifest.json",
    "clean:modules": "shx rm -rf node_modules",
    "clean:all": "yarn clean:dist && yarn clean:modules",
    "reinstall": "yarn clean:all && yarn install",
    "types:emit": "tsc -b",
    "build": "yarn clean:dist && yarn types:emit",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "clean:manifest",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": "https://github.com/JoaoTMDias/merge-coverage/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
